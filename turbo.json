{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "env": ["NODE_ENV", "NEXT_PUBLIC_URL"]
    },
    "topo": {
      "dependsOn": ["^topo"]
    },
    "//#lint": {
      "outputs": ["node_modules/.cache/.eslintcache"],
      "outputMode": "errors-only"
    },
    "lint": {
      "dependsOn": ["^topo"],
      "outputs": ["node_modules/.cache/.eslintcache"],
      "outputMode": "errors-only"
    },
    "//#lint:fix": {
      "dependsOn": ["^topo"],
      "outputs": ["node_modules/.cache/.eslintcache"],
      "outputMode": "errors-only"
    },
    "lint:fix": {
      "dependsOn": ["^topo"],
      "outputs": ["node_modules/.cache/.eslintcache"],
      "outputMode": "errors-only"
    },
    "typecheck": {
      "dependsOn": ["^topo"],
      "outputs": ["node_modules/.cache/tsbuildinfo.json"],
      "outputMode": "errors-only"
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "dependsOn": [],
      "persistent": true,
      "cache": false
    }
  }
}
