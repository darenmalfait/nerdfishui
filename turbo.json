{
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"],
			"env": ["NODE_ENV", "NEXT_PUBLIC_URL"]
		},
		"topo": {
			"dependsOn": ["^topo"]
		},
		"//#format": {
			"outputs": ["node_modules/.cache/prettiercache"]
		},
		"format": {
			"dependsOn": ["^topo"],
			"outputs": ["node_modules/.cache/prettiercache"]
		},
		"//#format:fix": {
			"outputs": ["node_modules/.cache/prettiercache"]
		},
		"format:fix": {
			"dependsOn": ["^topo"],
			"outputs": ["node_modules/.cache/prettiercache"]
		},
		"//#lint": {
			"outputs": ["node_modules/.cache/.eslintcache"]
		},
		"lint": {
			"dependsOn": ["^topo"],
			"outputs": ["node_modules/.cache/.eslintcache"]
		},
		"//#lint:fix": {
			"dependsOn": ["^topo"],
			"outputs": ["node_modules/.cache/.eslintcache"]
		},
		"lint:fix": {
			"dependsOn": ["^topo"],
			"outputs": ["node_modules/.cache/.eslintcache"]
		},
		"typecheck": {
			"dependsOn": ["^topo"],
			"outputs": ["node_modules/.cache/tsbuildinfo.json"]
		},
		"clean": {
			"cache": false
		},
		"dev": {
			"dependsOn": [],
			"persistent": true,
			"cache": false
		}
	}
}
